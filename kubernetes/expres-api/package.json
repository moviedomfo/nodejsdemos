{
  "name": "member-rapi-api",
  "version": "1.0.3",
  "description": "Express server that expose API to interactuate between  rapivox & SQL Express",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias && copy package.json dist",
    "builddocker": "tsc && tsc-alias && cp package.json dist",
    "buildlocal": "tsc && tsc-alias && copyfiles -u 1 src/**/*.json && copy package.json dist",
    "build-babel": "babel src --extensions \".ts,.js\" -d dist --copy-files",
    "cpfiles": "copyfiles -u 1 src/**/*.json dist",
    "restart": "rimraf dist && npm run start",
    "postinstall": "npm run tsc",
    "tsc": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "author": "moviedo",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "awilix": "^7.0.3",
    "awilix-express": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "env-var": "^7.5.0",
    "express": "^4.19.2",
    "express-handlebars": "^7.1.2",
    "express-jwt": "^8.4.1",
    "helmet": "^7.0.0",
    "jwks-rsa": "^2.0.5",
    "morgan": "^1.10.0",
    "openapi-types": ">=7",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.37.3",
    "sequelize-auto": "^0.8.8",
    "shallow-equal-object": "^1.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tedious": "^18.2.0",
    "tsoa": "^6.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-typescript": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@tsconfig/node14": "^14.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/swagger-ui-express": "^4.1.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.4.5"
  }
}